    "version": "tsds version"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.0.1",
    "node-version-use": "^2.1.5",
    "ts-dev-stack": "^1.21.3",
    "tsds-config": "^0.2.1"
  },
